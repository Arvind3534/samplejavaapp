pipeline{
    agent{
       label 'java'
    }
    tools{
    maven 'maven-1'
    }
    stages{
        stage('compile'){
            steps{
                sh "mvn clean compile"

            }
            
         }
        stage('junit'){
            steps{
                 junit 'target/surefire-reports/*.xml'
            }
            
        }
    }
}
